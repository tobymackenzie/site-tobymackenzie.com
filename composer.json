{
	"name": "tjm/site-tobymackenzie.com"
	,"autoload": {
		"psr-4": {
			"": "src/"
			,"TJM\\TMCom\\": "app/"
		}
		,"classmap": [
			"app/AppKernel.php"
		]
	}
	,"autoload-dev": {
		"files": ["vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"]
	}
	,"config": {
		"bin-dir": "bin"
	}
	,"extra": {
		"incenteev-parameters": {
			"file": "app/config/parameters.yml"
		}
		,"installer-paths": {
			"vendor/wp/plugin-{$name}": ["type:wordpress-plugin"]
			,"vendor/wp/theme-{$name}": ["type:wordpress-theme"]
		}
		,"symfony-app-dir": "app"
		,"symfony-assets-install": "relative"
		,"symfony-bin-dir": "bin"
		,"symfony-var-dir": "var"
		,"symfony-web-dir": "web"
		,"wordpress-install-dir": "vendor/wp/wordpress"
	}
	,"disabled": {
		"friendsofsymfony/http-cache-bundle": "~1.3.4"
	}
	,"minimum-stability": "dev"
	,"prefer-stable": true
	,"require": {
		"php": ">=5.5.9"
		,"symfony/symfony": "3.4.*"
		,"doctrine/orm": "^2.5"
		,"doctrine/doctrine-bundle": "^1.6"
		,"symfony/swiftmailer-bundle": "^2.3"
		,"symfony/monolog-bundle": "^3.1"
		,"symfony/polyfill-apcu": "^1.0"
		,"sensio/distribution-bundle": "^5.0"
		,"sensio/framework-extra-bundle": "^5.0.0"
		,"incenteev/composer-parameter-handler": "^2.0"
		,"twig/twig": "^1.0||^2.0"

		,"erusev/parsedown-extra": "~0.7"
		,"johnpbloch/wordpress": "^4.4"
		,"league/html-to-markdown": "^4.4"
		,"tjm/files": "master@dev"
		,"tjm/symfony-basebundle": "master@dev"
		,"tjm/symfony-standard-edition-bundle": "master@dev"
		,"tjm/utils": "master@dev"
		,"tjm/wpthemehelper": "0.1.x@dev"
		,"wpackagist-plugin/akismet": "^4.0"
		,"wpackagist-plugin/jetpack": "^5.0"
		,"wpackagist-plugin/syntaxhighlighter": "^3.2"
		,"wpackagist-plugin/wordpress-importer": "^0.6"
		,"wpackagist-theme/twentyfifteen": "^1.4"
	}
	,"require-dev": {
		"sensio/generator-bundle": "^3.0"
		,"symfony/phpunit-bridge": "^3.0"
	}
	,"repositories": [
		{
			"type": "git"
			,"url": "https://github.com/tobymackenzie/symfony-BaseBundle.git"
		}
		,{
			"type": "composer"
			,"url": "https://wpackagist.org"
		}
	]
	,"scripts": {
		"_symfony-scripts": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
			,"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
			,"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
			,"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
			,"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
			,"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
			,"bin/wp-install"
		]
		,"post-install-cmd": ["@_symfony-scripts"]
		,"post-update-cmd": ["@_symfony-scripts"]
	}
	,"type": "project"
}
